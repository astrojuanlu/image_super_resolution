   "metadata": {},
   "outputs": [],
   "source": [
    "input_imgs = orchest.get_inputs()[0]\n",
    "\n",
    "# \"predict\" each image\n",
    "rdn = RDN(weights='noise-cancel')\n",
    "output_imgs = [(name, rdn.predict(img)) for name, img in input_imgs]\n",
    "\n",
    "orchest.output(output_imgs)"
   ]
